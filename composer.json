{
    "name": "barryvdh/laravel-httpcache",
    "description": "HttpCache for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "httpcache",
        "cache",
        "esi"
    ],
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "illuminate/support": "^9|^10|^11.0",
        "symfony/console": "^6|^7.0",
        "symfony/http-kernel": "^6|^7.0",
        "barryvdh/laravel-stack-middleware": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7|^8|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\HttpCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Barryvdh\\HttpCache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        },
        "laravel": {
            "providers": [
                "Barryvdh\\HttpCache\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
